    "deploy": "npm run build-storybook && cd storybook-static && now --name react-hook-form-material-ui --prod"
  },
  "dependencies": {},
  "peerDependencies": {
    "@date-io/date-fns": "1.3.x",
    "@material-ui/core": "> 4.7 <5",
    "@material-ui/icons": "> 4.7 <5",
    "@material-ui/pickers": "3.2.x",
    "date-fns": "2.7.x",
    "react": ">=16.8",
    "react-hook-form": "> 5.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@shufflz/shufflo",
  "module": "dist/shufflo.esm.js",
  "devDependencies": {
    "@babel/core": "7.10.0",
    "@date-io/date-fns": "^1.3.13",
