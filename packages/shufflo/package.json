  "repository": "https://github.com/stackshuffle/shufflz/tree/master/packages/shufflo",
  "keywords": [
    "react",
    "mui",
    "material-ui",
    "react-hook-form"
  ],
  "main": "dist/shufflo.cjs.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "lint-fix": "tsdx lint --fix",
    "prepare": "tsdx build",
    "storybook": "start-storybook -p 6006",
